<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:UporabiskiVmesniki.resources">
    <Style x:Key="labelEna" TargetType="Label">
        <Setter Property="FontFamily" Value="Arial"></Setter>
        <Setter Property="FontWeight" Value="Bold"></Setter>
    </Style>
    <Style x:Key="labelDva" TargetType="Label">
        <Setter Property="FontFamily" Value="Tiems New Roman"></Setter>
        <Setter Property="FontWeight" Value="Medium"></Setter>
    </Style>
    <Style x:Key="labelTri" TargetType="Label">
        <Setter Property="Foreground" Value="Black"></Setter>
        <Setter Property="FontSize" Value="16"></Setter>
    </Style>

</ResourceDictionary>